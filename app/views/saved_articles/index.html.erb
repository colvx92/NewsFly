<% if @articles.count == 0 %>

  <h4>You don't have any saved articles</h4>

<% else %>

  <% @articles.each do |a| %>
    <%= link_to article["title"], article["url"] %><br>
    <%= link_to article["url"] do %>
      <%= image_tag article["urlToImage"] %>
    <% end unless article["urlToImage"].blank? %>
  <% end %>
